  "scripts": {
    "proinit:mkdir": "mkdirp dist docs src/scss/assets src/scss/assets/bootstrap src/scss/assets/bootstrap/honoka src/scss/assets/bootstrap/honoka/bootstrap src/scss/assets/bootstrap/honoka/bootstrap/mixins",
    "proinit:honoka1": "copyfiles -f \"./node_modules/bootstrap-honoka/scss/*.scss\" \"./src/scss/assets/bootstrap\"",
    "proinit:honoka2": "copyfiles -f \"./node_modules/bootstrap-honoka/scss/honoka/*.scss\" \"./src/scss/assets/bootstrap/honoka\"",
    "proinit:honoka3": "rename-files bootstrap.scss _bootstrap.scss --PATH=./src/scss/assets/bootstrap/",
    "proinit:bootstrap1": "copyfiles -f \"./node_modules/bootstrap-sass/assets/stylesheets/bootstrap/*.scss\" \"./src/scss/assets/bootstrap/honoka/bootstrap\"",
    "proinit:bootstrap2": "copyfiles -f \"./node_modules/bootstrap-sass/assets/stylesheets/bootstrap/mixins/*.scss\" \"./src/scss/assets/bootstrap/honoka/bootstrap/mixins\"",
    "proinit": "npm-run-all -s proinit:mkdir proinit:honoka1 proinit:honoka2 proinit:honoka3 proinit:bootstrap1 proinit:bootstrap2",
    "gueshingon": "npm run proinit:mkdir",
    "om": "npm run proinit:honoka1",
    "krodhana": "npm run proinit:honoka2",
    "hum": "npm run proinit:honoka3",
    "jah": "npm run proinit:bootstrap1",
    "svaha": "npm run proinit:bootstrap2",
    "gue": "npm-run-all -s gueshingon om krodhana hum jah svaha"
  },
